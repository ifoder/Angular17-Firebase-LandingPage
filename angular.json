{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "FireAdmin": {
      "projectType": "library",
      "root": "projects/fire-admin",
      "sourceRoot": "projects/fire-admin/src",
      "prefix": "fa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/fire-admin/tsconfig.lib.json",
            "project": "projects/fire-admin/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/fire-admin/src/test.ts",
            "tsConfig": "projects/fire-admin/tsconfig.spec.json",
            "karmaConfig": "projects/fire-admin/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/fire-admin/tsconfig.lib.json",
              "projects/fire-admin/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "website": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/web-app",
      "sourceRoot": "projects/web-app/src",
      "prefix": "wa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "public",
            "index": "projects/web-app/src/index.html",
            "browser": "projects/web-app/src/main.ts",
            "tsConfig": "./projects/web-app/tsconfig.app.json",
            "assets": [
              "./projects/web-app/src/favicon.ico",
              "./projects/web-app/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "./projects/web-app/src/theme.less",
              "./projects/web-app/src/styles.css",
              "./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "./node_modules/ng-zorro-antd-mobile/src/ng-zorro-antd-mobile.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "website:build:production"
            },
            "development": {
              "buildTarget": "website:build:development"
            }
          },
          "defaultConfiguration": "development"
        },

        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "./projects/web-app/tsconfig.spec.json",
            "assets": ["./projects/web-app/src/favicon.ico", "./src/assets"],
            "styles": [
              "./projects/web-app/src/styles.css",
              "./projects/web-app/node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "./node_modules/ng-zorro-antd-mobile/src/ng-zorro-antd-mobile.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/material-icons-font/material-icons-font.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/datatables.net-responsive-dt/css/responsive.dataTables.min.css",
              "node_modules/quill/dist/quill.snow.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/datatables.net/js/jquery.dataTables.min.js",
              "node_modules/datatables.net-responsive-dt/js/responsive.dataTables.min.js",
              "node_modules/chart.js/dist/Chart.min.js",
              "node_modules/shards-ui/dist/js/shards.min.js",
              "node_modules/quill/dist/quill.min.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "be429260-3840-40ed-91a0-d7942d1428a8"
  }
}
